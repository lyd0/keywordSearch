{
  "enable":[
    "baidu",
    "bing",
    "sogou"
  ],
  "rules":[
    {
      "baseURL":"http://cn.bing.com/search?",
      "queryParam":"q",
      "pageParam":"first",
      "pageMulti":10,
      "pageOffset":-10,
      "name":"bing",
      "itemCSS":"ol#b_results li[class=b_algo]",
      "titleCSS":"h2>a",
      "contentCSS":"p"
    },
    {
      "baseURL":"https://www.sogou.com/web?",
      "queryParam":"query",
      "pageParam":"page",
      "pageMulti":1,
      "pageOffset":0,
      "name":"sogou",
      "itemCSS":"div.rb,div.vrwrap",
      "titleCSS":"h3[class=vrTitle]>a",
      "contentCSS":"p,div.ft"
    },
    {
      "baseURL":"https://www.baidu.com/s?",
      "queryParam":"wd",
      "pageParam":"pn",
      "pageMulti":10,
      "pageOffset":-10,
      "name":"baidu",
      "itemCSS":"div#content_left div.result",
      "titleCSS":"h3[class=t]>a",
      "contentCSS":"div.c-row,div.c-abstract"
    }
  ]

}